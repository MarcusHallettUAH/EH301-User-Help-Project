{
    "build": {
        "content": [
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md",
                    "screencast/**.md",
                    "screencast/**/toc.yml"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "templates/branding"
        ],
        "postProcessors": [],
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    },
    "pdf": {
        "content": [
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md",
                    "pdf/*"
                ],
                "exclude": [
                    "**/bin/**",
                    "**/obj/**",
                    "_site_pdf/**",
                    "**/toc.yml",
                    "**/toc.md"
                ]
            },
            {
                "files": "pdf/toc.yml"
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ],
                "exclude": [
                    "**/bin/**",
                    "**/obj/**",
                    "_site_pdf/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "**/bin/**",
                    "**/obj/**",
                    "_site_pdf/**"
                ]
            }
        ],
        "dest": "_site_pdf"
    }
}